<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.province.postdoctor.mapper.dictionary.DictionaryMapper">

    <select id="department"   resultType="com.province.postdoctor.entity.dictionary.Dictionary">
        SELECT
            `name`
        FROM
            dictionary
        WHERE
	updictionaryId = '10001'
    </select>

    <select id="f_subject"   resultType="com.province.postdoctor.entity.dictionary.Dictionary">
        SELECT
            `name`
        FROM
            dictionary
        WHERE
	updictionaryId = '10003'
    </select>

    <select id="f_job"   resultType="com.province.postdoctor.entity.dictionary.Dictionary">
        SELECT
            `name`
        FROM
            dictionary
        WHERE
	updictionaryId = '10004'
    </select>

    <select id="s_job"   parameterType="com.province.postdoctor.entity.dictionary.Dictionary" resultType="com.province.postdoctor.entity.dictionary.Dictionary">
        SELECT
	`name`
FROM
	`dictionary`
WHERE
	updictionaryId IN (
		SELECT
			dictionaryId
		FROM
			`dictionary` WHERE `name`=#{name})
    </select>


</mapper>
